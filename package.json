{
	"name": "tds-replay",
	"displayName": "TDS Replay for VSCode",
	"description": "%tds.replay.package.description%",
	"version": "0.0.1",
	"publisher": "TOTVS",
	"author": {
		"name": "TOTVS S.A.",
		"email": "tecnologia@totvs.com.br"
	},
	"license": "LGPL-3.0 ",
	"keywords": [
		"protheus advpl logix vscode-extension"
	],
	"engines": {
		"vscode": "^1.29.0",
		"node": "^8.9.3"
	},
	"icon": "images/tds-replay.png",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/totvs/tds-replay-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/totvs/tds-replay-vscode/issues"
	},
	"scripts": {
		"prepare": "tsc -p ./src",
		"compile": "tsc -p ./src",
		"tslint": "tslint ./src/**/*.ts",
		"watch": "tsc -w -p ./src",
		"test": "mocha -u tdd ./out/tests/",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"dependencies": {
		"@totvs/tds-da": "^1.0.17",
		"await-notify": "1.0.1",
		"cheerio": "^1.0.0-rc.3",
		"fs": "0.0.1-security",
		"strip-json-comments": "^2.0.1",
		"template-literal": "^1.0.3",
		"tmp": "^0.1.0",
		"vscode-nls": "^4.1.0"
	},
	"devDependencies": {
		"@types/mocha": "5.2.5",
		"@types/node": "8.9.3",
		"del": "^4.1.1",
		"gulp": "^4.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-tslint": "^8.1.4",
		"gulp-typescript": "^5.0.1",
		"gulp-util": "^3.0.8",
		"mocha": "5.2.0",
		"tslint": "5.11.0",
		"typescript": "3.1.6",
		"vsce": "1.53.0",
		"vscode": "^1.1.34",
		"vscode-nls-dev": "^3.2.6"
	},
	"main": "./out/extension.js",
	"activationEvents": [
		"onView:tds-replay.explorer"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "TDS Replay",
			"properties": {
				"tdsreplay.welcomePage": {
					"scope": "window",
					"type": "boolean",
					"default": true,
					"description": "%tds.replay.package.show.welcome.first.initialize%"
				}
			}
		},
		"commands": [
			{
				"command": "tds-replay.load",
				"title": "%tds.replay.package.load%",
				"icon": {
					"dark": "resources/dark/load.png",
					"light": "resources/light/load.png"
				},
				"category": "TDS Replay"
			},
			{
				"command": "tds-replay.record",
				"title": "%tds.replay.package.record%",
				"icon": {
					"dark": "resources/dark/add.png",
					"light": "resources/light/add.png"
				},
				"category": "TDS Replay"
			},
			{
				"command": "tds-replay.play",
				"title": "%tds.replay.package.play%",
				"category": "TDS Replay"
			},
			{
				"command": "tds-replay.showWelcome",
				"title": "%tds.replay.package.show.welcome.first.initialize%",
				"category": "TDS Replay"
			}
		],
		"keybindings": [],
		"breakpoints": [],
		"debuggers": [],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tds-replay",
					"title": "TDS Replay",
					"icon": "images/tdsreplay24x24.png"
				}
			]
		},
		"views": {
			"tds-replay": [
				{
					"id": "tds-replay.explorer",
					"name": "%tds.replay.package.titleView%"
				}
			]
		},
		"menus": {
			"explorer/context": [],
			"view/item/context": [],
			"view/title": [
				{
					"command": "tds-replay.load"
				},
				{
					"command": "tds-replay.record",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"command": "tds-replay.load"
				},
				{
					"command": "tds-replay.record"
				},
				{
					"command": "tds-replay.play"
				},
				{
					"command": "tds-replay.showWelcome"
				}
			]
		}
	}
}
